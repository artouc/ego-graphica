{
    "name": "@egographica/shared",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./types/*": {
            "types": "./src/types/*.ts",
            "import": "./src/types/*.ts"
        },
        "./messages/*": {
            "types": "./src/messages/*.ts",
            "import": "./src/messages/*.ts"
        },
        "./schemas/*": {
            "types": "./src/schemas/*.ts",
            "import": "./src/schemas/*.ts"
        }
    },
    "scripts": {
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts"
    },
    "dependencies": {
        "zod": "^3.24.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "typescript": "^5.6.0"
    }
}
